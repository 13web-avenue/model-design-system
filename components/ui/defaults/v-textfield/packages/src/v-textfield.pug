v-text-field( :value="value"
                :label="label ? label : 'Regular'"
                :outline="outline ? outline : false"
                :color="color ? color : 'primary'"
                :disabled="disabled ? disabled : false"
                :readonly="readonly ? readonly : false"
                :loading="loading ? loading : false"
                :rules="rules"
                :class="className"
                :type="type ? type : 'text'"
            )